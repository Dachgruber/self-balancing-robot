@book{AndrewS.Tanenbaum.2009,
 author = {{Andrew S. Tanenbaum}},
 year = {2009},
 title = {Moderne Betriebssysteme},
 urldate = {2023-07-10},
 edition = {3},
 publisher = {{Person Studium}},
 location = {M{\"u}nchen},
 abstract = {Der Vogel-Strau{\ss}-Algorithmus ist ein Begriff aus der Informatik. Wenn mehrere Threads auf Grund zyklischer Bedingungen zwischen Betriebsmitteln in eine Verklemmung (engl. Deadlock) kommen, gibt es verschiedene M{\"o}glichkeiten, diese zu behandeln.

Der Vogel-Strau{\ss}-Algorithmus stellt quasi den einfachsten Ansatz dar: Die Tatsache, dass ein Deadlock aufgetreten ist, wird einfach ignoriert. Als Konsequenz daraus folgt, dass dieser Deadlock nie aufgel{\"o}st wird -- die Threads warten bis in alle Unendlichkeit auf die Betriebsmittel und werden irgendwann {\glqq}von au{\ss}en{\grqq} aus dem Zustand befreit (im Regelfall, indem man sie abbricht). {\glqq}Vogel-Strau{\ss}-Algorithmus{\grqq} ist somit einfach eine abf{\"a}llige Bezeichnung daf{\"u}r, ein Problem nicht zu l{\"o}sen.

Aus technischer Sicht ist die Verhinderung oder automatische Aufl{\"o}sung von Verklemmungen der richtige Weg und diese Form der {\glqq}Problembehandlung{\grqq} indiskutabel. Wenn jedoch der Aufwand daf{\"u}r zu gro{\ss} ist, oder die Wahrscheinlichkeit, dass eine Verklemmung eintritt, zu klein, bzw. eine Verklemmung nur gezielt durch menschliches Eingreifen herbeigef{\"u}hrt werden kann, kann diese Vorgehensweise sinnvoll sein.

Der Name des Algorithmus stammt vom Vogel Strau{\ss}, der einem falschen Ger{\"u}cht nach bei Gefahr den Kopf in den Sand steckt, um das {\glqq}Problem{\grqq} zu ignorieren.},
 file = {Wikipedia (Ed.) 2020 - Vogel-Strau{\ss}-Algorithmus:Attachments/Wikipedia (Ed.) 2020 - Vogel-Strau{\ss}-Algorithmus.pdf:application/pdf}
}


